{
  "rules": {
    "no-console": 0,
    "comma-dangle": [2, "never"],//对象字面量项尾不能有逗号
    "linebreak-style": [0, "windows"],
    "no-extend-native": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "no-eval": 0,
    "no-useless-constructor":0,
    "no-underscore-dangle": 0,
    "global-require": 0
  },
  "parser": "babel-eslint"
}