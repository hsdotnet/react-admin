{
    "presets": [ "react", "es2015", "stage-2" ],
    "plugins": [
        "syntax-dynamic-import",
        "transform-runtime",
        "babel-plugin-add-module-exports",
        "transform-decorators-legacy",
        "transform-class-properties",
        "react-hot-loader/babel",
        [
            "import",
            {
                "libraryName": "antd",
                "style": true
            }
        ]
    ],
    "comments": false
}